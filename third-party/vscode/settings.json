{
    "diffEditor.renderSideBySide": true,
    "editor.cursorBlinking": "phase",
    "editor.fontLigatures": true,
    "editor.fontFamily": "Fira Code, Operator Mono, Menlo, Monaco, Consolas, 'Courier New', monospace",
    "editor.formatOnSave": true,
    "editor.formatOnSaveTimeout": 2000,
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": true
    },
    "editor.rulers": [
        80,
        100,
        120
    ],
    "explorer.openEditors.visible": 0,
    "files.autoSave": "onFocusChange",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/vendor/*/**": true
    },
    "git.autofetch": true,
    "git.confirmSync": false,
    "jenkins.pipeline.linter.connector.url": "https://stc81mgmtxv001.marconet.com/pipeline-model-converter/validate",
    "jenkins.pipeline.linter.connector.crumbUrl": "https://stc81mgmtxv001.marconet.com/crumbIssuer/api/xml?xpath=concat(//crumbRequestField,%22:%22,//crumb)",
    "jenkins.pipeline.linter.connector.user": "nate.morris",
    "jenkins.pipeline.linter.connector.strictssl": false,
    "powershell.codeFormatting.preset": "OTBS",
    "powershell.powerShellExePath": "C:\\Windows\\system32\\WindowsPowerShell\\v1.0\\powershell.exe",
    "prettier.printWidth": 120,
    "prettier.trailingComma": "es5",
    "vetur.format.styleInitialIndent": true,
    "vetur.format.scriptInitialIndent": true,
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Cobalt2",
    "workbench.startupEditor": "welcomePage",
    "xmlTools.splitAttributesOnFormat": true,
    "[javascript]": {
        "editor.insertSpaces": true,
        "editor.autoIndent": true
    },
    "[vue]": {
        "editor.insertSpaces": true,
        "editor.autoIndent": true
    },
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.autoIndent": false
    }
}
